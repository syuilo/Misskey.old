doctype html

html
	head
		include templates/head
		title Misskey Developer Center
		link(rel='stylesheet', href='/resources/styles/dev.css')
		script(src='/resources/scripts/dev.js')
	body
		include templates/header
		
		header
			div#titleLogo
				h1
					i.fa.fa-terminal
					| Developer-Center
		div#contents
			nav
				ul
					li
						h1
							i.fa.fa-puzzle-piece
							| My Applications
						if apps != null
							each app in apps
								p= app.name
					li
						h1 REST API
						p Item1
					li
						h1 MenuItem
			main
		footer
			small (c)Misskey.